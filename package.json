    "less-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "postcss-px2rem-exclude": "0.0.6",
    "sass-loader": "^7.1.0",
    "scp2": "^0.5.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-template-compiler": "2.5.10",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px2rem-exclude": {
        "remUnit": 128,
        "exclude": "/node_modules/i"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
