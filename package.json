    "muse-ui-toast": "^0.3.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "promise-polyfill": "^8.1.0",
    "vue": "2.5.10",
    "vue-backtotop": "^1.6.1",
    "vue-loading-spinner": "^1.0.11",
    "vue-router": "^3.0.6",
    "vue2-transitions": "^0.3.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
